<!DOCTYPE html>
<html>
<head>
  <% include ../partials/head %>
</head>
<body class="container">

  <header>
    <% include ../partials/header %>
  </header>

  <main>
    <div class="jumbotron">
      <h1>This is great</h1>
      <p>Welcome to templating using EJS</p>

      <h2>Tagline</h2>
      <%= tagline %>

      <h2>Loop</h2>
      <% drinks.forEach(function(drink){ %>
        <li><%= drink.name %> - <%= drink.drunkness %> </li>
      <% }); %>
    </div>
  </main>

  <footer>
    <% include ../partials/footer %>
  </footer>

</body>
</html>